{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@now/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/main.py"
    }
  ],
  "env": {
    "FLASK_ENV": "production",
    "FLASK_APP": "main.py"
  }
}
